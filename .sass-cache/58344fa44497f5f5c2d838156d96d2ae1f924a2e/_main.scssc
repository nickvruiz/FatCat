/* About Page Styling */
#aboutblock{
    bottom:-100%;
    background: white;
    header{
        position: relative;
        width: 100%;
        height: 120px;
        line-height: 120px;
        text-align: center;
        background: $darkgrey;
        h1{
            margin: 0;
            padding: 0;
            color: white;
            font-size: 4.929em;
            font-style: italic;
        }
        a{
            display: block;
            position: absolute;
            height: 80px;
            width: 20px;
            top: 0;
            right: 0;
            margin: 0;
            padding: 20px;
            color: white;
            font-weight: 200;
            font-size: 2em;
            line-height: 80px;
            background: $mediumgrey;
            cursor: pointer;
        }
    }

    // Mission statement
    .content{
        width: 61%;
        margin: 0 auto;
        font-weight: 200;
        font-size: 2.243em;
        line-height: 55px;
        color: $darkgrey;
        p{
            margin: 60px 0;
            padding:0;
        }
        span{
            color: $blue;
            font-weight: 400;
            font-style: italic;
        }
        .line{
            @include line(100px);
        }
    }
}


/* Nav Button */
nav{
    margin: 20px 0 0 20px;
    position: fixed;
    top: 0;
    left: 0;
    z-index: 999;
    .menu{
        display: block;
        width: 70px;
        height: 70px;
        position: relative;
        margin-right: 20px;
        float: left;
        text-align: center;
        line-height: 70px;
        z-index: 4;
        .list{
            position: absolute;
            width: 26px;
            height: 26px;
            top: 50%;
            left: 50%;
            margin: -11px 0 0 -13px;
            color: white;
            font-size: 1.15em;
            text-align: center;
            line-height: 23px;
            border-radius: 100%;
            background: none;
            &:hover{
                color: $blue;
            }
        }
        .circle{
            width: 100%;
            height: 100%;
            position: absolute;
            top: 0;
            left: 0;
            text-align: center;
            line-height: 100%;
            z-index: -1;
            span{
                position: absolute;
                display: block;
                width: 30px;
                height: 30px;
                top: 50%;
                left: 50%;
                margin: -14px 0 0 -14px;
                padding: 0;
                line-height: 100%;
                border-radius: 100%;
                // border: 2px solid transparent;
                background: none;
            }
            .menu-active{
                width: 70px;
                height: 70px;
                margin: -34px 0 0 -34px;
                background: $lightgrey;
                // border: 2px solid #333;
            }
        }
        &:hover,
        &:active{
            color: $blue;
            cursor: pointer;
        }
        a{
            display: block;
            width: inherit;
            height: inherit;
            text-indent: -999em;
        }
    }


    /* Rotating text */
    .flip{
        float: left;
        height: 70px;
        color: $grey;
        line-height: 70px;
        font-size: 1.769em;
        font-weight: 200;
        word-spacing: 9px;
        p{
            margin: 0;
        }
        span{
            display: inline-block;
            margin: 0;
            padding: 0;
            width: 55px;
            text-align: center;
            a{
                color: $blue;
                line-height: 24px;
                border-bottom: 1px dotted $darkgrey;
                &:hover{
                    color: $grey;
                    cursor: pointer;
                }
            }
        }
    }


    /* Navigation links */
    ul#items{
        width: 70px;
        margin: 0;
        padding: 0;
        color: white;
        text-align: center;
        li.navItem{
            position: relative;
            list-style: none;
            //display: none;
            button{
                display: block;
                width: 22px;
                margin: 0 auto;
                padding: 25px 0;
                color: $lightgrey;
                border: none;
                background: none;
                border-bottom: 1px solid $darkgrey;
                overflow: hidden;
                &:hover{
                    color: white;
                }
            }
            &:last-child{
                button{ border-bottom: none; }
            }
            span{
                display: none;
                position: absolute;
                width: 80px;
                left: 50px;
                padding: 10px;
                color: #888;
                text-align: left;
            }

            // Insert nav icons
            // .home   { background: url('../img/nav-icon-home.png')    no-repeat center; }
            // .work   { background: url('../img/nav-icon-work.png')    no-repeat center; }
            // .contact{ background: url('../img/nav-icon-contact.png') no-repeat center; }
            // .about  { background: url('../img/nav-icon-about.png')   no-repeat center; }
        }
    }
