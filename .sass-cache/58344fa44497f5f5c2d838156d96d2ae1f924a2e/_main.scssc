/*
#  - Main
#  - Work
#  - Home
#  - About
*/


/* Main Window */
#main{
    position: relative;
    width: 100%;
    height: 100%;
    overflow: hidden;
}

/* Contact Page */
#contactblock{
    display: none;
    position: absolute;
    width: 100%;
    height: 100%;
    margin: 0;
    padding: 0;
    top: 0;
    left: 0;
    background: url('../img/wrapper-bg.png') repeat top left;
    opacity: 0;
    z-index: 0;
    header{
        position: absolute;
        top: 20px;
        right: 20px;
    }
}

.wrapper{
    position: absolute;
    width: 50%;
    height: 700px;
    top: 50%;
    left: 25%;
    margin-top: -227px;
    h1{
        margin: 0 0 30px 0;
        color: white;
        text-transform: uppercase;
    }
}

#content{
    position: absolute;
    width: 100%;
    height: 100%;
    // top: 100%!important;
    top: 0;
    left: 0;
}

#workblock,
#homeblock,
#aboutblock{
    position: absolute;
    width: 100%;
    height: 100%;
    left: 0;
    margin: 0; padding: 0;
}


/* Home Page Styling */
#homeblock{
    top: 0;
    background: url('../img/bg.jpg') no-repeat center center fixed;

    //Full size background
    -webkit-background-size: cover;
       -moz-background-size: cover;
             -o-background-size: cover;
                background-size: cover;

    //Fluid center
    .logo{
        width: 373px;
        height: 156px;
        position: absolute;
        top: 50%;
        left: 50%;
        margin: -78px 0 0 -186.5px;
        overflow: hidden;
        text-indent: -999em;
        background: url('../img/logo-fatcat.png') no-repeat center;
        opacity: 0;
        h1, small{
            margin-left: -999em;
        }
    }
}


/* Work Page Styling */
#workblock{
    top: -100%;
    background: $grey;
}

.portfolio-sidebar{
    width: 15%;
    height: 100%;
    float: left;
    background: #1a1a1a;
    h3{
        color: #c8c8c8;
        text-indent: 40px;
        margin: 0;
        padding: 40px 0 20px 0;
        border-top: 1px solid #333;
    }
}

.portfolio-sidebar-box{
    width: 100%;
    margin-top: 95px;
    border-top: 5px solid black;
}

ul.portfolio-categories{
    margin: 0;
    padding: 0;
    li{
        width: 100%;
        height: 50px;
        margin: 0 0 1px 0;
    }
    a{
        display: block;
        width: 100%;
        height: 50px;
        line-height: 50px;
        color: $darkgrey;
        font-size: 1.286em;
        text-transform: uppercase;
        font-weight: 100;
        text-indent: 40px;
    }
    .active, a:hover{
        position: relative;
        color: white;
        background: #282828;
        &:before{
            content: '';
            position: absolute;
            width: 5px;
            height: 100%;
            background: #333;
            top: 0;
            left: 0;
        }
    }
}

ul.stage{
    width: 85%;
    height: 100%;
    margin: 0;
    padding: 0;
    float: left;
    overflow-x: hidden;
    overflow-y: scroll;
    li{
        list-style: none;
        width: 50%;
        height: 50%;
        overflow: hidden;
        float: left;
        text-align: center;
        img{
            width: 100%;
            height: auto;
        }
    }
}
